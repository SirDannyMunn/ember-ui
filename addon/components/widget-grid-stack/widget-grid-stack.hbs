<GridStack
    @options={{this.args.options}}
    @onDragstart={{this.dragStart}}
    @onDragstop={{this.dragStop}}
    @onResizestart={{this.resizeStart}}
    @onResizestop={{this.resizeStop}}
    @onAdded={{this.added}}
    @onChange={{this.change}}
    @onEnable={{this.enable}}
    @onRemoved={{this.remove}}
    as |grid|
>
    {{yield}}
</GridStack>